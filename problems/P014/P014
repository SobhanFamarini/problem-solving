def merge():
    List=[]
    List2=[]
    n=int(input("Number:"))
    for x in range(1, n+1):
        t=(input("text:"))
        List.append(t)
    m=int(input("Number:"))
    for i in range(1, m+1):
        t2=(input("text:"))
        List2.append(t2)
    List.extend(List2)
    return(List)
print(merge())