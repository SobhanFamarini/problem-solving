t=input("whould you like to start with sample input?(y) or (n):")
if t=="y":
    print("Number:4")
    n=4
else:
    n=int(input("Number:"))
z=0
x=1
while x>0:
    x+=1
    m=0
    for i in range(2,x):
        if x%i==0:
            m+=1
    if m==0:
        z+=1
        if z==n:
            break
print(x)